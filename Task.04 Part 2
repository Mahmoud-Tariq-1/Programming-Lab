using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        Console.Write("Enter the number you to convert : ") ;
        int num = Convert.ToInt32(Console.ReadLine()) ; //num is the number inputed by the user
        if(num<10) // the condition allows only input less than 10 to be converted
        {
            if (num == 1)
            {
            Console.WriteLine("One") ;
            }
            else if (num == 2)
            {
                Console.WriteLine("Two") ;
            }
            else if (num == 3)
            {
                Console.WriteLine("Three") ;
            }
            else if (num == 4)
            {
                Console.WriteLine("Four") ;
            }
            else if (num == 5)
            {
                Console.WriteLine("Five") ;
            }
            else if (num == 6)
            {
                Console.WriteLine("Six") ;
            }
            else if (num == 7)
            {
                Console.WriteLine("Seven") ;
            }
            else if (num == 8)
            {
                Console.WriteLine("Eight") ;
            }
            else if (num == 9)
            {
                Console.WriteLine("Nine") ;
            }
            else if (num == 10)
            {
                Console.WriteLine("Ten") ;
            }
        }
       else // else condition gives any input more than 10 to output "The number you entered is too big"
       {
           Console.WriteLine("The number you entered is Too Big") ;
       }
    }
}
